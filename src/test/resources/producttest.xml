<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Market" verbose="1">

    <test name="Smoke of market">
        <groups>
            <define name="smoke"/>
            <define name="regression"/>
            <run>
                <include name="smoke"/>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="JUnit.ProductMarketTest"/>
        </classes>
    </test>

</suite>
